:root {
    // html的样式
    // 背景颜色
    --wang-bgcolor: #f5f5f5;
    // 菜单字体颜色
    --wang-menu-color: #fffff;
    // 所有字体颜色
    --wang-text-color: #000000;
    // 菜单悬浮颜色
    --wang-meun-hover-color: #000044;

    // 菜单背景颜色
    --wang-menu-bgcolor: #f4f4f4;

    --wang-color-primary-lighter: #5c9eea;
    --wang-color-success-lighter: #f0f9eb;
    --wang-color-warning-lighter: #fdf6ec;
    --wang-color-danger-lighter: #fef0f0;
    --wang-color--parimary-bg: #d6e9fb;

    --wang-bglefts: #1f84e2;
}
html,
body,
#app {
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    font-family: Helvetica Neue, Helvetica, PingFang SC, Hiragino Sans GB, Microsoft YaHei, SimSun, sans-serif;
    font-weight: 400;
    -webkit-font-smoothing: antialiased;
    -webkit-tap-highlight-color: transparent;
    background-color: var(--wang-bgcolor);
    font-size: 14px;
    overflow: hidden;
    position: relative;
}
ul {
    list-style: none;
}
* {
    padding: 0;
    margin: 0;
}
